recursive sql

set @id := 21;                        #node id
set @L := 0;

SELECT @r AS _ID, (SELECT @r := parent_id FROM domain WHERE id = _ID) AS _PARENT_ID, (@L := @L + 1) AS level
FROM ( SELECT @r := @id) AS vars, (SELECT * FROM domain WHERE id < @id) AS h
WHERE @r is not null;

